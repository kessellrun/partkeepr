<?xml version="1.0" ?>
<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <parameters>
        <parameter key="partkeepr_api_doc.extractor.api_doc_extractor.class">PartKeepr\ApiDocBundle\Extractor\ApiDocExtractor</parameter>
    </parameters>

    <services>
        <service id="nelmio_api_doc.extractor.api_doc_extractor"
                 class="%partkeepr_api_doc.extractor.api_doc_extractor.class%">
            <argument type="service" id="service_container"/>
            <argument type="service" id="router"/>
            <argument type="service" id="annotation_reader"/>
            <argument type="service" id="nelmio_api_doc.doc_comment_extractor"/>
            <argument type="service" id="nelmio_api_doc.controller_name_parser"/>
            <argument type="collection"/>
            <argument type="collection"/>
        </service>
    </services>
</container>
